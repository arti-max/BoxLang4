$ifndef cli_incl
$define cli_incl 1

namespace cli (

    box putc[char c] -> void (
        asm["psh (c)"];
        asm["int $2"];
    )

    box puts[char* s] -> void (
        while [*s != (char)0] (
            open putc[*s];
            s:s+1;
        )
    )

    box print_nl[] -> void (
        open putc[(char)0xA];
    )

)

$endif